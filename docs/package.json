{
    "name": "docs",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy": "pnpm run build && wrangler pages deploy",
        "dev": "vite dev",
        "format": "prettier --write .",
        "generate-social": "tsx scripts/generate-social-images.ts",
        "lint": "prettier --check . && eslint .",
        "lint:fix": "pnpm run format && eslint . --fix",
        "preview": "vite preview",
        "sitemap:manifest": "node ./scripts/generate-sitemap-manifest.mjs",
        "test": "pnpm run test:unit -- --run && pnpm run test:e2e",
        "test:e2e": "playwright test",
        "test:unit": "vitest"
    },
    "overrides": {
        "@sveltejs/kit": {
            "cookie": "^0.7.0"
        }
    },
    "dependencies": {
        "@humanspeak/svelte-motion": "^0.1.5",
        "@sentry/cloudflare": "^10.15.0",
        "@sentry/sveltekit": "^10.15.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250924.0",
        "@eslint/compat": "^1.4.0",
        "@humanspeak/svelte-virtual-list": "workspace:*",
        "@internationalized/date": "^3.9.0",
        "@lucide/svelte": "^0.544.0",
        "@playwright/test": "^1.55.1",
        "@sveltejs/adapter-auto": "^6.1.0",
        "@sveltejs/adapter-cloudflare": "^7.2.3",
        "@sveltejs/kit": "^2.43.4",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.13",
        "@types/eslint": "^9.6.1",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "autoprefixer": "^10.4.21",
        "bits-ui": "^2.11.4",
        "clsx": "^2.1.1",
        "embla-carousel-svelte": "^8.6.0",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-svelte": "^3.12.4",
        "eslint-plugin-unused-imports": "^4.2.0",
        "formsnap": "^2.0.1",
        "globals": "^16.4.0",
        "highlight.js": "^11.11.1",
        "lucide-svelte": "^0.544.0",
        "mdsvex": "^0.12.6",
        "mode-watcher": "^1.1.0",
        "paneforge": "^1.0.2",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-sort-json": "^4.1.1",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "puppeteer": "^24.22.3",
        "shiki": "^3.13.0",
        "svelte": "^5.39.6",
        "svelte-check": "^4.3.2",
        "svelte-sonner": "^1.0.5",
        "sveltekit-superforms": "^2.27.1",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.1",
        "tailwindcss": "^4.1.13",
        "tsx": "^4.20.5",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.1",
        "vaul-svelte": "1.0.0-next.7",
        "vite": "^7.1.7",
        "vitest": "^3.2.4",
        "wrangler": "^4.42.0",
        "zod": "^4.0.15"
    },
    "volta": {
        "node": "22.16.0"
    }
}
